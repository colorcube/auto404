{
  "name": "colorcube/auto404",
  "type": "typo3-cms-extension",
  "description": "Page Not Found handling without configuration. Handle redirect to other domains when pages were moved.",
  "keywords": [
    "TYPO3",
    "extension",
    "auto404",
    "Page Not Found",
    "404",
    "301",
    "redirect"
  ],
  "license": "GPL-2.0-or-later",
  "homepage": "https://github.com/colorcube/auto404",
  "support": {
    "issues": "https://github.com/colorcube/auto404/issues"
  },
  "authors": [
    {
      "name": "RenÃ© Fritz",
      "email": "r.fritz@colorcube.de",
      "role": "Developer",
      "homepage": "http://www.colorcube.de"
    }
  ],
  "require": {
    "php": ">=7.0.0",
    "typo3/cms-core": "^7.6 || ^8.0"
  },
  "autoload": {
    "psr-4": {
      "Colorcube\\Auto404\\": "Classes"
    }
  },
  "replace": {
    "typo3-ter/auto404": "self.version"
  },
  "extra": {
    "typo3/cms": {
      "extension-key": "auto404"
    }
  }
}
